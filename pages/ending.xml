<page id="ending" bg_image="tech-bg-lab.png" bg_music="osts-vilo.mp3">
  <agent id="hero" x="20" y="330" image="forest-hero-idle.png" width="100" />

  <agent id="door_open" x="600" y="340" image="forest-door-1.png" width="100" />

  <fab-for each="i in 10">
    <agent
      id="ground-$i"
      image="desert-ground-2.png"
      x="$i * 80"
      y="520"
      width="80"
      animation="slide-right"
    />
  </fab-for>

  <agent
    id="next_page_text"
    text="Primeira Pagina"
    animation="glitch"
    y="480"
    x="640"
    on_click="goToPage('start2')"
  />
</page>

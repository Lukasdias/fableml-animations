<page id="ending" bg_image="tech-bg-lab.png" bg_music="osts-vilo.mp3">
  <agent
    id="teste"
    text="Hello World Ending"
    color="white"
    x="center"
    y="center"
    font-size="70"
    on_click="set(abriu_porta, !$abriu_porta)"
  />

  <fab-if when="$abriu_porta">
    <agent id="text" font-size="20" text="text" color="white" />
  </fab-if>

  <fab-for each="i in 10">
    <agent id="ground-$i" image="desert-ground-1.png"
        x="$i * 80"
        width="80"
      />
  </fab-for>

  <agent
    id="next_page_text"
    text="Primeira Pagina"
    animation="glitch"
    y="480"
    x="640"
    on_click="goToPage('start2')"
  />
</page>

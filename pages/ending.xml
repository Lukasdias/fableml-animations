<page id="ending" bg_image="tech-bg-lab.png" bg_music="osts-vilo.mp3">
  <agent
    id="teste"
    text="Hello World Ending"
    color="white"
    x="center"
    y="center"
    font-size="70"
    on_click="set(abriu_porta, true)"
  />

  <fab-if when="$abriu_porta">
    <agent id="text" text="text" color="white" />
  </fab-if>

  <agent id="button" x="center" y="10" font-size="30" color="white" />

  <agent
    id="next_page_text"
    text="Primeira Pagina"
    animation="glitch"
    y="480"
    x="640"
    on_click="goToPage('start2')"
  />
</page>
